<!--menu.wxml-->
<view class="page">
    <block wx:for="{{intro}}" wx:key="id">
      <navigator url="../list/list?motto={{item.title}}" class="weui-cell weui-cell_access">
        <view class="page__hd">
          <view class="page__title">{{item["title"]}}</view>
          <view class="page__desc">{{item["desc"]}}</view>
        </view>
      </navigator>
    </block>
</view>

<!--menu.wxml-->
<view class="page">
    <button>清单广场</button>
    <block wx:for="{{intro}}" wx:key="id">
      <navigator url="/pages/{{item['page']}}/{{item['page']}}" class="weui-cell weui-cell_access">
        <view class="page__hd">
          <view class="page__title">{{item["title"]}}</view>
          <view class="page__desc">{{item["desc"]}}</view>
        </view>
      </navigator>
    </block>
</view>

<view class="page">
    <block wx:for="{{intro}}" wx:key="title" wx:for-index="id">
      <navigator id="{{id}}" url="../list/list?id={{id}}" class="weui-cell weui-cell_access" bindtouchstart="touchS" bindtouchmove="touchM" bindtouchend="touchE" >
        <view class="page__hd" >
          <view style="{{item.txtStyle}};{{item.titleColor}}" class="page__title">{{item["title"]}}</view>
          <view style="{{item.txtStyle}}" class="page__desc">{{item["desc"]}}</view>
        </view>

